{
  "name": "@tunarr/shared",
  "version": "0.18.11",
  "description": "Utility functions shared between server and web",
  "private": true,
  "keywords": [],
  "license": "Zlib",
  "author": "chrisbenincasa",
  "type": "module",
  "scripts": {
    "bundle": "tsup",
    "build": "tsc -p tsconfig.prod.json",
    "build-dev": "tsup --dts --watch",
    "clean": "rimraf ./build/",
    "dev": "tsc --watch --declaration",
    "test": "vitest"
  },
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "default": "./build/index.js"
    },
    "./constants": {
      "types": "./build/constants/index.d.ts",
      "default": "./build/constants/index.js"
    },
    "./util": {
      "types": "./build/util/index.d.ts",
      "default": "./build/util/index.js"
    },
    "./types": {
      "types": "./build/types/index.d.ts"
    }
  },
  "main": "index.ts",
  "dependencies": {
    "@tunarr/types": "workspace:*",
    "dayjs": "^1.11.10",
    "lodash-es": "^4.17.21",
    "random-js": "2.1.0",
    "tslib": "^2.6.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/lodash-es": "4.17.9",
    "@types/node": "22.10.7",
    "rimraf": "^5.0.5",
    "ts-essentials": "^9.4.1",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^3.0.3"
  },
  "peerDependencies": {
    "ts-pattern": "^5.4.0"
  }
}
