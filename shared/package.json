{
  "name": "@tunarr/shared",
  "version": "0.22.1",
  "description": "Utility functions shared between server and web",
  "private": true,
  "keywords": [],
  "license": "Zlib",
  "author": "chrisbenincasa",
  "type": "module",
  "scripts": {
    "bundle": "tsup",
    "build": "tsup --dts",
    "build-dev": "tsup --dts --watch",
    "clean": "rimraf ./dist/",
    "dev": "tsup --dts --watch",
    "test": "vitest --run"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "default": "./dist/constants/index.js"
    },
    "./util": {
      "types": "./dist/util/index.d.ts",
      "default": "./dist/util/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts"
    }
  },
  "main": "index.ts",
  "dependencies": {
    "@tunarr/types": "workspace:*",
    "chevrotain": "^11.0.3",
    "dayjs": "^1.11.14",
    "lodash-es": "^4.17.21",
    "random-js": "2.1.0",
    "tslib": "^2.6.2",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@rollup/plugin-swc": "^0.4.0",
    "@types/lodash-es": "4.17.9",
    "@types/node": "22.10.7",
    "@vitest/coverage-v8": "^3.2.4",
    "rimraf": "^5.0.5",
    "ts-essentials": "^9.4.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "ts-pattern": "^5.4.0"
  }
}
