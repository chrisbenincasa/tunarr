    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.10",
    "@types/node": "18.18.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3",
    "ts-essentials": "^9.4.1"
