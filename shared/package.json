{
  "name": "@tunarr/shared",
  "version": "0.1.0",
  "description": "Utility functions shared between server and web",
  "keywords": [],
  "license": "Zlib",
  "author": "chrisbenincasa",
  "type": "module",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "default": "./build/index.js"
    },
