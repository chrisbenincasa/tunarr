    "prettier": "^3.5.1",
    "rimraf": "^5.0.5",
    "tar": "^7.4.3",
    "thread-stream": "^3.1.0",
    "tmp": "^0.2.1",
    "tmp-promise": "^3.0.3",
    "ts-essentials": "^10.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typed-emitter": "^2.1.0",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.19.0",
    "vitest": "^3.2.4"
  }
}
